    <editor:PropertyField binding-path="showAreaGizmos" name= "show gizmos" tooltip="this will show the collider bounds of the transition area (light gray) and the interior area (dark gray) in the scene view"/>
    <editor:PropertyField binding-path="transitionArea" name="transition-area" tooltip=""/>
    <editor:PropertyField binding-path="interiorArea" name="interior-area" tooltip=""/>
  </engine:Foldout>
</engine:UXML>